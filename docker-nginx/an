server {
    listen 80;
    location / {
        root /var/www/dist;
        index index.html index.htm;
        try_files $uri $uri/ /index.html;
    }

    location /api/ {
        include proxy_params;
        proxy_pass http://an-container:5000/;
    }

    location /static/ {
        alias /an/static/;
    }
}